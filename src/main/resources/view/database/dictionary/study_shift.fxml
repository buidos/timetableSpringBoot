<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<VBox alignment="TOP_CENTER" maxHeight="1.7976931348623157E308" prefHeight="414.0" prefWidth="842.0" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="bel.dmitrui98.timetable.controller.database.dictionary.StudyShiftController">
   <children>
      <HBox alignment="TOP_CENTER" maxWidth="-Infinity" minWidth="-Infinity" prefHeight="31.0" prefWidth="678.0" spacing="10.0">
         <children>
            <VBox prefHeight="52.0" prefWidth="159.0">
               <children>
                  <Label text="Название:" />
                  <TextField fx:id="studyShiftNameTextField" />
               </children>
            </VBox>
            <VBox prefHeight="52.0" prefWidth="113.0">
               <children>
                  <Label text="Значение:" />
                  <TextField fx:id="studyShiftValueTextField" promptText="integer" />
               </children>
            </VBox>
            <Button fx:id="defaultButton" defaultButton="true" mnemonicParsing="false" onAction="#addStudyShift" text="Добавить">
               <HBox.margin>
                  <Insets top="20.0" />
               </HBox.margin></Button>
            <Button mnemonicParsing="false" onAction="#editStudyShift" text="Редактировать">
               <HBox.margin>
                  <Insets top="20.0" />
               </HBox.margin></Button>
            <Button mnemonicParsing="false" onAction="#deleteStudyShift" text="Удалить">
               <HBox.margin>
                  <Insets top="20.0" />
               </HBox.margin></Button>
         </children>
         <opaqueInsets>
            <Insets right="10.0" />
         </opaqueInsets>
      </HBox>
      <HBox maxWidth="-Infinity" prefWidth="678.0">
         <children>
            <Label fx:id="editIndexLabel" prefHeight="21.0" prefWidth="701.0" text="Номер для редактирования:">
               <padding>
                  <Insets left="41.0" />
               </padding></Label>
         </children>
      </HBox>
      <HBox layoutX="139.0" layoutY="51.0" maxWidth="-Infinity" prefWidth="678.0">
         <children>
            <Label fx:id="deleteIndexesLabel" text="Номера для удаления:">
               <padding>
                  <Insets left="41.0" />
               </padding></Label>
         </children>
      </HBox>
      <TableView fx:id="studyShiftTableView" maxHeight="1.7976931348623157E308" maxWidth="-Infinity" prefHeight="325.0" prefWidth="834.0">
         <columnResizePolicy>
            <TableView fx:constant="CONSTRAINED_RESIZE_POLICY" />
         </columnResizePolicy>
         <columns>
            <TableColumn fx:id="studyShiftIndexCol" maxWidth="600.0" prefWidth="97.0" text="№" />
            <TableColumn fx:id="studyShiftNameCol" prefWidth="715.0" text="Название" />
            <TableColumn fx:id="studyShiftValueCol" prefWidth="715.0" text="Значение" />
         </columns>
      </TableView>
   </children>
   <padding>
      <Insets left="10.0" right="10.0" top="10.0" />
   </padding>
</VBox>
